#include <iostream>
using namespace std;
int main() {
    int arr[5] = {1, 2, 3, 4, 5};
    int sum = 0;
    cout << "Array elements: ";
    for (int i = 0; i < 5; i++) {
        cout << arr[i] << " ";
        sum += arr[i];
    }
    cout << endl;
    cout << "Sum of array elements: " << sum << endl;
    return 0;
}
